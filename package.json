{
  "name": "fitbot",
  "version": "1.0.0",
  "description": "Chatbot that helps manage caloric intake vs. exercise goals",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "document-readme": "documentation readme ./server -s \"API DOCS\" && documentation readme ./dialogFlowFulfillment ./fitBotLambda/intentHandlers ./fitbotWatsonCall -s \"FULFILLMENT HANDLERS\" && documentation readme ./client -s \"FRONT END DOCS\"",
    "heroku-token": "script/encrypt-heroku-auth-token",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "prepare": "if [ -d .git ]; then npm-merge-driver install; fi",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "BOT='LEX' node server",
    "start-dev-dialog-flow": "BOT='DIALOG_FLOW' NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server-dialog-flow",
    "start-server-dialog-flow": "BOT='DIALOG_FLOW' nodemon server -e html,js,scss --ignore public --ignore client",
    "start-dev-watson": "BOT='WATSON' NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server-watson",
    "start-server-watson": "BOT='WATSON' nodemon server -e html,js,scss --ignore public --ignore client",
    "start-dev-lex": "BOT='LEX' NODE_ENV='development' npm run build-client-watch & BOT='LEX' NODE_ENV='development' npm run start-server-lex",
    "start-server-lex": "BOT='LEX' nodemon server -e html,js,scss --ignore public --ignore client",
    "start-dev-rasa": "BOT='RASA' NODE_ENV='development' npm run build-client-watch & BOT='RASA' NODE_ENV='development' npm run start-server-rasa",
    "start-server-rasa": "BOT='RASA' nodemon server -e html,js,scss --ignore public --ignore client",
    "unit": "BOT='DIALOG_FLOW' NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" \"./fitbotLambda/**/*.spec.js\" \"./dialogFlowFulfillment/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "unit-lex": "BOT='LEX' NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" \"./fitbotLambda/**/*.spec.js\" \"./dialogFlowFulfillment/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "unit-watson": "BOT='WATSON' NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" \"./fitbotLambda/**/*.spec.js\" \"./dialogFlowFulfillment/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "end2end-watson": "NODE_ENV='test' BOT='WATSON' nightwatch",
    "end2end-lex": "NODE_ENV='test' BOT='LEX' nightwatch",
    "end2end-dialog-flow": "NODE_ENV='test' BOT='DIALOG_FLOW' nightwatch",
    "test": "npm run unit && npm run unit-lex && npm run unit-watson"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Isaias Pomales",
  "license": "MIT",
  "dependencies": {
    "actions-on-google": "^2.5.0",
    "aws-sdk": "^2.814.0",
    "axios": "^0.15.3",
    "chart.js": "^2.8.0",
    "compression": "^1.7.3",
    "connect-session-sequelize": "^4.1.0",
    "dialogflow": "^0.7.0",
    "dialogflow-fulfillment": "^0.6.1",
    "express": "^4.16.3",
    "express-session": "^1.15.1",
    "history": "^4.6.3",
    "morgan": "^1.9.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "pluralize": "^7.0.0",
    "prop-types": "^15.6.2",
    "randomstring": "^1.1.5",
    "react": "^16.4.2",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.3.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.38.0",
    "socket.io": "^2.1.1",
    "watson-developer-cloud": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.54",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.54",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.54",
    "@babel/plugin-proposal-function-sent": "7.0.0-beta.54",
    "@babel/plugin-proposal-json-strings": "7.0.0-beta.54",
    "@babel/plugin-proposal-numeric-separator": "7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.0.0-beta.54",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.54",
    "@babel/plugin-syntax-import-meta": "7.0.0-beta.54",
    "@babel/polyfill": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "@babel/register": "^7.0.0-beta.55",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.4",
    "chai": "^3.5.0",
    "chromedriver": "^2.45.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-fullstack": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "nightwatch": "^1.0.18",
    "nodemon": "^1.18.3",
    "npm-merge-driver": "^2.3.5",
    "prettier": "1.11.1",
    "react-test-renderer": "^16.4.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "supertest": "^3.1.0",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  }
}
